[ Command
    { command'id = 1
    , command'always = False
    , command'method = "POST"
    , command'url = [ Right "/" ]
    , command'request_body =
        Just (JsonObject (Object (fromList [ ( "a" , JsonInteger 0 ) ])))
    , command'response_body =
        Just
          (JsonCombine
             (JsonCustom "r" (Left (Ref 1 Request [] (fromList []))) :|
                [ JsonObject
                    (Object
                       (fromList
                          [ ( "b"
                            , JsonCustom "r" (Left (Ref 1 Request [ Right "a" ] (fromList [])))
                            )
                          ]))
                ]))
    }
]
