[ Command
    { command'id = 1
    , command'always = False
    , command'method = "POST"
    , command'url = [ Right "/" ]
    , command'request_body =
        Just
          (JsonObject
             (Object
                (fromList
                   [ ( "x"
                     , JsonArray
                         [ JsonObject
                             (Object
                                (fromList
                                   [ ( "a" , JsonInteger 0 )
                                   , ( "b" , JsonInteger 1 )
                                   , ( "c" , JsonInteger 2 )
                                   ]))
                         ]
                     )
                   ])))
    , command'response_body =
        Just
          (JsonCombine
             (JsonCustom
                "r"
                (RespRef
                   (Ref 1 Request [ Right "x" , Left 0 ] (fromList [ "a" , "b" ]))) :|
                [ JsonObject
                    (Object
                       (fromList
                          [ ( "b"
                            , JsonCustom
                                "r" (RespRef (Ref 1 Request [ Right "a" ] (fromList [])))
                            )
                          ]))
                ]))
    }
]
